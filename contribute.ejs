 <!-- Contribute Products -->


 <div class="container marketing my-4 text-center">
  <div class="col-md-12 my-3">
    <h3 class="featurette-heading fw-normal lh-1 pb-3">Please fill in the form below to add your product to our Hub.</h3>
    <!-- <p class="lead">Please fill in the form below to add your product to our Hub.</p>-->
  </div>
  
  <h5>Product Details</h5>

  <div class="container col-xs-12 col-sm-6 col-md-12 text-center">
    <form class="needs-validation" novalidate="" action="/submit" method="post">
      <div class="row g-3">
        <div class="col-sm-12 col-xs-12 mt-3">
          <label for="firstName" class="form-label">Product Name:</label>
          <input type="text" class="form-control" id="firstName" name="productName" placeholder="" value="" required="">
          <div class="invalid-feedback">
            Valid product name is required.
  </div>

        <div class="col-sm-12 mt-3">
          <label for="productDescription" class="form-label">Product Description:</label>
          <input type="text" class="form-control" id="productDescription" name="productDescription" placeholder="" value="" required="">
          <div class="invalid-feedback">
            Valid description is required.
          </div>
        </div>

<br />
      <h5>Contact Details</h5>

        <div class="col-12 mt-3">
          <label for="email" class="form-label">Email Address</label>
          <input type="email" class="form-control" id="email" name="emailAddress" placeholder="you@gov.com">
          <div class="invalid-feedback">
            Please enter a valid email address.
          </div>
        </div>

        <div class="col-12 mt-3">
          <label for="phoneNumber" class="form-label">Phone Number <span class="text-body-secondary">(Optional)</span></label>
          <input type="text" class="form-control" id="phoneNumber" name="phoneNumber" placeholder="+65 91234567">
          <div class="invalid-feedback">
            Please enter a valid phone number.
          </div>
        </div>


      <hr class="my-4">

      <div class="form-check">
        <input type="checkbox" class="form-check-input" id="PDPA">
        <label class="form-check-label" for="PDPA">PDPA</label>
      </div>

      <hr class="my-4">

      <button class="w-100 btn btn-primary btn-lg" type="submit" onclick="">Submit</button>

    </form>
  </div>
</div>
</div>
